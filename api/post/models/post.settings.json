{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "Slug": {
      "type": "string",
      "unique": true,
      "required": true
    }
  }
}
